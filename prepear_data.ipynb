    "   \"source\": [\n",
    "    \"\\n\",\n",
    "    \"def process_text_and_build_csv_rows(text: str):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Обрабатывает текст и возвращает список строк CSV\\n\",\n",
    "    \"    (без заголовка)\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    translate_str, accad_str = '', ''\\n\",\n",
    "    \"    # next_pos, close_pos = 0, 0\\n\",\n",
    "    \"    extract_function_1 = [extract_quoted_substring, extract_letter_space_digit_colon_space]\\n\",\n",
    "    \"    extract_function_2 = [extract_parenthesized_substring, extract_single_quotes]\\n\",\n",
    "    \"    # str_txt = [translate_str, accad_str]\\n\",\n",
    "    \"    # str_txt_1 = [accad_str, translate_str]\\n\",\n",
    "    \"    str_txt = [\\\"\\\", \\\"\\\"]\\n\",\n",
    "    \"    str_txt_1 = [\\\"\\\", \\\"\\\"]\\n\",\n",
    "    \"    # pos_num = [next_pos, close_pos]\\n\",\n",
    "    \"    # pos_num_1 = [close_pos, next_pos]\\n\",\n",
    "    \"    # pos_num = [next_pos, next_pos]\\n\",\n",
    "    \"    # pos_num_1 = [close_pos, close_pos]\\n\",\n",
    "    \"    # len_arr = len(str_txt)\\n\",\n",
    "    \"    len_arr = 1\\n\",\n",
    "    \"    i = 0\\n\",\n",
    "    \"    csv_rows = []\\n\",\n",
    "    \"    start_pos = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # while start_pos < len(text):\\n\",\n",
    "    \"    while i < len_arr:\\n\",\n",
    "    \"        # поиск по двойным кавычкам потом по буквам пробелам цифрам\\n\",\n",
    "    \"        # str_txt[i % len_arr], flag, pos_num[i % len_arr] = extract_function_1[i % len_arr](text, start_pos)\\n\",\n",
    "    \"        str_txt[i % len_arr], flag, next_pos = extract_function_1[i % len_arr](text, start_pos)\\n\",\n",
    "    \"        # if flag:\\n\",\n",
    "    \"        #     double_txt, double_flag, double_next_pos = extract_function_1[i % len_arr](text, next_pos)\\n\",\n",
    "    \"        # if translate_str is None:\\n\",\n",
    "    \"        #     break\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if flag:\\n\",\n",
    "    \"            if i == 0:\\n\",\n",
    "    \"                tx = \\\"Перевод \\\"\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                tx = \\\"Транслитерация \\\"\\n\",\n",
    "    \"            print(f\\\"{tx}{str_txt[i % len_arr]}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # print(translate_str)\\n\",\n",
    "    \"            # поиск по круглым скобкам потом по одинарным кавычкам\\n\",\n",
    "    \"            # str_txt_1[i % len_arr], flag2, pos_num_1[i % len_arr] = extract_function_2[i % len_arr](text, pos_num[i % len_arr])\\n\",\n",
    "    \"            str_txt_1[i % len_arr], flag2, close_pos = extract_function_2[i % len_arr](text, next_pos)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if flag2:\\n\",\n",
    "    \"                double_txt, double_flag, double_next_pos = extract_function_1[i % len_arr](text, next_pos)\\n\",\n",
    "    \"                if double_next_pos < close_pos and double_flag:\\n\",\n",
    "    \"                    str_txt[i % len_arr] = double_txt\\n\",\n",
    "    \"                match i:\\n\",\n",
    "    \"                    case 0:\\n\",\n",
    "    \"                        translate_str = str_txt[i % len_arr]\\n\",\n",
    "    \"                        accad_str = str_txt_1[i % len_arr]\\n\",\n",
    "    \"                        tx = \\\"Транслитерация \\\"\\n\",\n",
    "    \"                    case 1:\\n\",\n",
    "    \"                        translate_str = str_txt_1[i % len_arr]\\n\",\n",
    "    \"                        accad_str = str_txt[i % len_arr]\\n\",\n",
    "    \"                        tx = \\\"Перевод \\\"\\n\",\n",
    "    \"                print(f\\\"{tx}{str_txt_1[i % len_arr]}\\\")\\n\",\n",
    "    \"                # 1. Очистка перевода\\n\",\n",
    "    \"                t = translate_str.replace(\\\"\\\\n\\\", \\\" \\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # 2. Очистка аккадского\\n\",\n",
    "    \"                a = accad_str.replace(\\\"\\\\n\\\", \\\" \\\")\\n\",\n",
    "    \"                a = normalize_for_mt(a)\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # 3. Токенизация перевода\\n\",\n",
    "    \"                t_sentences = sent_tokenize(t)\\n\",\n",
    "    \"                t_sentences = [sent for sent in t_sentences if looks_like_real_translation(sent)]\\n\",\n",
    "    \"                # определение языка и перевод на английский, если перевод не английский\\n\",\n",
    "    \"                t_sentences = [translate_to_english(sent) if detect_language(sent) != 'en' else sent for sent in t_sentences]\\n\",\n",
    "    \"                # 4. Выравнивание + маркеры\\n\",\n",
    "    \"                a = align_and_mark_sentences(a, t_sentences, marker=\\\"<sent>\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # 5. Склеиваем перевод обратно\\n\",\n",
    "    \"                t = \\\" \\\".join(t_sentences)\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # 6. CSV-экранирование (ОДИН РАЗ!)\\n\",\n",
    "    \"                a = a.replace('\\\"', '\\\"\\\"')\\n\",\n",
    "    \"                t = t.replace('\\\"', '\\\"\\\"')\\n\",\n",
    "    \"\\n\",\n",
    "    \"                csv_rows.append(f'\\\"{a}\\\",\\\"{t}\\\"\\\\n')\\n\",\n",
    "    \"                # start_pos = pos_num_1[i % len_arr] + 1\\n\",\n",
    "    \"                start_pos = close_pos + 1\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                # start_pos = pos_num[i % len_arr] + 1\\n\",\n",
    "    \"                # start_pos = next_pos + 1\\n\",\n",
    "    \"                # start_pos = len(text)\\n\",\n",
    "    \"                i += 1\\n\",\n",
    "    \"                start_pos = 0\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            # start_pos = pos_num[i % len_arr] + 1\\n\",\n",
    "    \"            # start_pos = next_pos + 1\\n\",\n",
    "    \"            # start_pos = len(text)\\n\",\n",
    "    \"            i += 1\\n\",\n",
    "    \"            start_pos = 0\\n\",\n",
    "    \"        # if start_pos >= len(text):\\n\",\n",
    "    \"        #     i += 1\\n\",\n",
    "    \"        #     start_pos = 0\\n\",\n",
    "    \"    return csv_rows\\n\"\n",
