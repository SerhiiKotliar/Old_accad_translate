    "   \"source\": [\n",
    "    \"def extract_quoted_substring(text: str, start_pos: int):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Ищет в строке text, начиная С позиции start_pos,\\n\",\n",
    "    \"    подстроку вида: ' \\\"текст\\\"'.\\n\",\n",
    "    \"    Возвращает:\\n\",\n",
    "    \"        (substring, is_longer_than_30, closing_quote_pos)\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    if start_pos != 0:\\n\",\n",
    "    \"        start_pos += 1\\n\",\n",
    "    \"    # 1. Основной шаблон\\n\",\n",
    "    \"    pattern = re.compile(\\n\",\n",
    "    \"        r'/k \\\\d{4}:'\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    match = pattern.search(text, start_pos)\\n\",\n",
    "    \"    if not match:\\n\",\n",
    "    \"        return None, None, start_pos\\n\",\n",
    "    \"    start_pos = match.end()\\n\",\n",
    "    \"    translate = False\\n\",\n",
    "    \"    open_seq = ' \\\"'\\n\",\n",
    "    \"    # if start_pos != 0:\\n\",\n",
    "    \"    #     start_pos += 1\\n\",\n",
    "    \"    # поиск начинается С start_pos\\n\",\n",
    "    \"    open_pos = text.find(open_seq, start_pos)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if open_pos == -1:\\n\",\n",
    "    \"        return None, None, start_pos\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # позиция открывающей кавычки \\\"\\n\",\n",
    "    \"    quote_start = open_pos + 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # ищем закрывающую кавычку \\\"\\n\",\n",
    "    \"    quote_end = text.find('\\\"', quote_start + 1)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if quote_end == -1:\\n\",\n",
    "    \"        return None, None, start_pos\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # подстрока между кавычками\\n\",\n",
    "    \"    substring = text[quote_start + 1 : quote_end]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    dash_count = substring.count('-')\\n\",\n",
    "    \"    aleph_count = substring.count('ℵ')\\n\",\n",
    "    \"    # dash_required = len(substring) / 50\\n\",\n",
    "    \"    if dash_count > 0 or aleph_count > 0:\\n\",\n",
    "    \"        if dash_count > 0:\\n\",\n",
    "    \"            dash_required = len(substring) / dash_count\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            dash_required = 34\\n\",\n",
    "    \"        # много символов транслитерации\\n\",\n",
    "    \"        if dash_required < 25 or aleph_count > 0:\\n\",\n",
    "    \"            return None, None, start_pos\\n\",\n",
    "    \"    if len(substring) > 30:\\n\",\n",
    "    \"        translate = True\\n\",\n",
    "    \"\\n\",\n",
    "    \"    return substring, translate, quote_end\\n\"\n",
