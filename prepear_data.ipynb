    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def looks_like_real_translation(text, min_len=10):\\n\",\n",
    "    \"    \\\"\\\"\\\"Проверка: текст реально перевод, а не транслитерация/номер/каталог\\\"\\\"\\\"\\n\",\n",
    "    \"    # if not text or not isinstance(text, str):\\n\",\n",
    "    \"    #     return False\\n\",\n",
    "    \"    text = text.strip()\\n\",\n",
    "    \"    if len(text) < min_len:\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"    if \\\".\\\" not in text:\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"    digit_ratio = sum(c.isdigit() for c in text) / len(text)\\n\",\n",
    "    \"    # if digit_ratio > 0.15:\\n\",\n",
    "    \"    if digit_ratio > 0.3:\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"    return True\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Чтобы langdetect всегда возвращал один и тот же результат для одного текста\\n\",\n",
    "    \"DetectorFactory.seed = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"def detect_language(text):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Определяет язык текста.\\n\",\n",
    "    \"    Возвращает код языка, например: 'en', 'fr', 'de', 'ru'\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        lang = detect(text)\\n\",\n",
    "    \"        return lang\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"Не удалось определить язык: {e}\\\")\\n\",\n",
    "    \"        return None\\n\",\n",
    "    \"\\n\",\n",
    "    \"def translate_to_english(text):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Переводит текст на английский, если язык не английский.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    lang = detect_language(text)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if not lang:\\n\",\n",
    "    \"        # Если язык не определён, возвращаем оригинальный текст\\n\",\n",
    "    \"        return text\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if lang != 'en':\\n\",\n",
    "    \"        try:\\n\",\n",
    "    \"            translated_text = GoogleTranslator(source=lang, target='en').translate(text)\\n\",\n",
    "    \"            return translated_text\\n\",\n",
    "    \"        except Exception as e:\\n\",\n",
    "    \"            print(f\\\"Ошибка перевода: {e}\\\")\\n\",\n",
    "    \"            return text\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        # Если текст уже на английском\\n\",\n",
    "    \"        return text\\n\",\n",
    "    \"\\n\",\n",
    "    \" # texts = [\\n\",\n",
    "    \" #        \\\"Bonjour, comment ça va?\\\",           # французский\\n\",\n",
    "    \" #        \\\"Привет, как дела?\\\",                 # русский\\n\",\n",
    "    \" #        \\\"This text is already English.\\\",     # английский\\n\",\n",
    "    \" #        \\\"Hola, ¿cómo estás?\\\"                 # испанский\\n\",\n",
    "    \" #    ]\\n\",\n",
    "    \" #\\n\",\n",
    "    \" #    for t in texts:\\n\",\n",
    "    \" #        print(\\\"Оригинал:\\\", t)\\n\",\n",
    "    \" #        translated = translate_to_english(t)\\n\",\n",
    "    \" #        print(\\\"Перевод :\\\", translated)\\n\",\n",
    "    \" #        print(\\\"-\\\" * 50)\"\n",
    "   ],\n",
